<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="com.aptana.ide.feature.doc"
      label="%featureName"
      version="2.0.0.qualifier"
      provider-name="%providerName">

   <description>
      %description
   </description>

   <copyright url="%copyrightURL">
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <url>
      <update label="%updateSiteName" url="http://update.aptana.com/update/studio/"/>
   </url>

   <requires>
      <import plugin="org.eclipse.core.runtime" version="3.2.0" match="compatible"/>
      <import plugin="org.eclipse.core.resources" version="3.2.0" match="compatible"/>
      <import plugin="org.eclipse.jface" version="3.2.0" match="compatible"/>
      <import plugin="org.eclipse.ui.ide" version="3.2.0" match="compatible"/>
      <import plugin="org.eclipse.ui.browser"/>
      <import plugin="org.eclipse.help.base" version="3.2.0" match="compatible"/>
      <import plugin="org.eclipse.help.ui"/>
      <import plugin="com.aptana.ide.core"/>
      <import plugin="com.aptana.ide.core.ui"/>
      <import plugin="com.aptana.ide.intro"/>
      <import plugin="com.aptana.ide.server.jetty"/>
      <import plugin="com.aptana.ide.libraries.jetty"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.jface.text"/>
      <import plugin="org.eclipse.ui.workbench.texteditor"/>
      <import plugin="com.aptana.ide.editors"/>
   </requires>

   <plugin
         id="com.aptana.ide.documentation"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="com.aptana.ide.snippets"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="com.aptana.ide.samples"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="com.aptana.ide.samples.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
