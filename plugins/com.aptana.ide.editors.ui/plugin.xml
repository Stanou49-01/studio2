<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="com.aptana.ide.js.ui"
            class="com.aptana.ide.editors.ui.views.problems.ProblemsView"
            icon="icons/warning.png"
            id="com.aptana.ide.js.ui.views.problemsView"
            name="%Validation.view.name">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="com.aptana.ide.editors.preferences.GeneralPreferencePage"
            class="com.aptana.ide.editors.internal.ui.preferences.TodoPreferencePage"
            id="com.aptana.ide.editors.ui.tasks"
            name="Tasks">
      </page>
   </extension>

</plugin>
