            commandId="org.eclipse.ui.edit.text.toggleShowWhitespaceCharacters"
            contextId="com.aptana.ide.editors.UnifiedEditorsScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+."/>

      <key
		    commandId="com.aptana.ide.editors.openDeclarationAction"
            contextId="com.aptana.ide.editors.UnifiedEditorsScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="F3"/>      
      <key
            commandId="org.eclipse.ui.edit.text.shiftLeft"
            contextId="org.eclipse.ui.textEditorScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M3+ARROW_LEFT"/>
      <key
            commandId="org.eclipse.ui.edit.text.shiftRight"
            contextId="org.eclipse.ui.textEditorScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M3+ARROW_RIGHT"/>
