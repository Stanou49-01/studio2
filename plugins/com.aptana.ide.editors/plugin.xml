   <extension
         point="org.eclipse.ui.bindings">
         
      <key
		    commandId="com.aptana.ide.editors.showDocAction"
            contextId="com.aptana.ide.editors.UnifiedEditorsScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="F2"/>
