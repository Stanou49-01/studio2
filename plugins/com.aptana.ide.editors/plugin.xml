            commandId="com.aptana.ide.editors.quickOutlineAction"
            contextId="com.aptana.ide.editors.UnifiedEditorsScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+O">
      </key>
      
      <key
            commandId="com.aptana.ide.editors.nextMultipageEditorTab"
            contextId="com.aptana.ide.editors.UnifiedEditorsScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+TAB">
      </key>
      <key
            commandId="com.aptana.ide.editors.previousMultipageEditorTab"
            contextId="com.aptana.ide.editors.UnifiedEditorsScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M2+TAB">
      </key>
