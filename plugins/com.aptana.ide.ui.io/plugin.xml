<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.core.expressions.propertyTesters">
      <propertyTester
            class="com.aptana.ide.ui.io.internal.FileSystemObjectPropertyTester"
            id="com.aptana.ide.ui.io.navigator.filePropertyTester"
            namespace="com.aptana.ide.ui.io.navigator.file"
            properties="isDirectory, isLocal"
            type="com.aptana.ide.ui.io.navigator.FileSystemObject">
      </propertyTester>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="false"
            id="com.aptana.ide.ui.io.connectionPointContribution"
            objectClass="com.aptana.ide.core.io.IConnectionPoint">
         <action
               class="com.aptana.ide.ui.io.actions.ConnectionPropertiesAction"
               enablesFor="1"
               id="com.aptana.ide.ui.io.actions.connectionProperties"
               label="%connectionPropertiesAction.label"
               menubarPath="group.properties">
         </action>
         <action
               class="com.aptana.ide.ui.io.actions.DisconnectAction"
               icon="icons/full/etool16/disconnect.png"
               id="com.aptana.ide.ui.io.actions.DisconnectAction"
               label="%disconnectAction.label"
               menubarPath="group.edit">
         <enablement>
            <objectState
                  name="canDisconnect"
                  value="true">
            </objectState>
         </enablement>
         </action>
         <action
               class="com.aptana.ide.ui.io.actions.DeleteConnectionAction"
               enablesFor="+"
               icon="icons/full/etool16/delete.png"
               id="com.aptana.ide.ui.io.actions.DeleteConnectionAction"
               label="%deleteAction.label"
               menubarPath="group.edit">
         </action>
      </objectContribution>
      <objectContribution
            adaptable="false"
            id="com.aptana.ide.ui.io.localCategoryContribution"
            objectClass="com.aptana.ide.core.io.IConnectionPointCategory">
         <action
               class="com.aptana.ide.ui.io.actions.NewLocalConnectionAction"
               enablesFor="1"
               icon="icons/full/etool16/add.png"
               id="com.aptana.ide.ui.io.actions.newLocalConnection"
               label="%newLocalConnectionAction.label"
               menubarPath="group.new">
         </action>
         <visibility>
            <objectState
                  name="isCategory"
                  value="com.aptana.ide.core.io.localShortcuts">
            </objectState>
         </visibility>
      </objectContribution>
      <objectContribution
            adaptable="false"
            id="com.aptana.ide.ui.io.workspaceCategoryContribution"
            objectClass="com.aptana.ide.core.io.IConnectionPointCategory">
         <action
               class="com.aptana.ide.ui.io.actions.NewWorkspaceConnectionAction"
               enablesFor="1"
               icon="icons/full/etool16/add.png"
               id="com.aptana.ide.ui.io.actions.newWorkspaceConnection"
               label="%newWorkspaceConnectionAction.label"
               menubarPath="group.new">
         </action>
         <visibility>
            <objectState
                  name="isCategory"
                  value="com.aptana.ide.core.io.projectShortcuts">
            </objectState>
         </visibility>
      </objectContribution>
      <objectContribution
            adaptable="false"
            id="com.aptana.ide.ui.io.genericCategoryContribution"
            objectClass="com.aptana.ide.core.io.IConnectionPointCategory">
         <action
               class="com.aptana.ide.ui.io.actions.NewGenericConnectionAction"
               enablesFor="1"
               icon="icons/full/etool16/add.png"
               id="com.aptana.ide.ui.io.actions.newGenericConnection"
               label="%newGenericConnectionAction.label"
               menubarPath="group.new">
         </action>
         <visibility>
            <objectState
                  name="isCategory"
                  value="com.aptana.ide.core.io.generic">
            </objectState>
         </visibility>
      </objectContribution>
      <objectContribution
            adaptable="false"
            id="com.aptana.ide.ui.io.filesystemObjectContribution"
            objectClass="com.aptana.ide.ui.io.navigator.FileSystemObject">
         <action
               class="com.aptana.ide.ui.io.actions.PromoteToProjectAction"
               icon="icons/full/etool16/folder_project.gif"
               id="com.aptana.ide.ui.io.actions.PromoteToProjectAction"
               label="%action.label.PromoteToProject"
               menubarPath="group.studio"
               style="push"
               tooltip="%action.tooltip.PromoteToProject">
         </action>
         <visibility>
            <objectState
                  name="isLocal"
                  value="true">
            </objectState>
         </visibility>
      </objectContribution>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="true"
            category="com.aptana.ide.js.ui"
            class="com.aptana.ide.ui.io.navigator.FileNavigatorView"
            icon="icons/full/eview16/file_explorer.png"
            id="com.aptana.ide.ui.io.fileExplorerView"
            name="%fileExplorerView.name">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.viewer">
      <viewer
            viewerId="com.aptana.ide.ui.io.fileExplorerView">
         <popupMenu
               allowsPlatformContributions="true"
               id="com.aptana.ide.ui.io.fileExplorerView#PopupMenu">
            <insertionPoint
                  name="group.new">
            </insertionPoint>
            <insertionPoint
                  name="group.open"
                  separator="true">
            </insertionPoint>
            <insertionPoint
                  name="group.openWith">
            </insertionPoint>
            <insertionPoint
                  name="group.edit"
                  separator="true">
            </insertionPoint>
            <insertionPoint
                  name="group.reorganize">
            </insertionPoint>
            <insertionPoint
                  name="group.port"
                  separator="true">
            </insertionPoint>
            <insertionPoint
                  name="group.build"
                  separator="true">
            </insertionPoint>
            <insertionPoint
                  name="group.generate"
                  separator="true">
            </insertionPoint>
            <insertionPoint
                  name="group.search"
                  separator="true">
            </insertionPoint>
            <insertionPoint
                  name="group.studio"
                  separator="true">
            </insertionPoint>
            <insertionPoint
                  name="additions"
                  separator="true">
            </insertionPoint>
            <insertionPoint
                  name="group.properties"
                  separator="true">
            </insertionPoint>
         </popupMenu>
      </viewer>
      <viewerContentBinding
            viewerId="com.aptana.ide.ui.io.fileExplorerView">
         <includes>
            <contentExtension
                  pattern="org.eclipse.ui.navigator.resources.filters.*">
            </contentExtension>
            <contentExtension
                  pattern="org.eclipse.ui.navigator.resources.linkHelper">
            </contentExtension>
            <contentExtension
                  pattern="org.eclipse.ui.navigator.resources.workingSets">
            </contentExtension>
            <contentExtension
                  pattern="com.aptana.ide.ui.io.navigatorContent">
            </contentExtension>
            <contentExtension
                  pattern="com.aptana.ide.ui.io.navigator.filters.*">
            </contentExtension>
         </includes>
      </viewerContentBinding>
       <viewerActionBinding
            viewerId="com.aptana.ide.ui.io.fileExplorerView">
         <includes>
             <actionExtension pattern="org.eclipse.ui.navigator.resources.*" />
             <actionExtension
                   pattern="com.aptana.ide.ui.io.navigator.actions.*">
             </actionExtension>
         </includes>
       </viewerActionBinding>
       <dragAssistant
             class="org.eclipse.ui.navigator.resources.ResourceDragAdapterAssistant"
             viewerId="com.aptana.ide.ui.io.fileExplorerView"/>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <navigatorContent
            activeByDefault="true"
            contentProvider="com.aptana.ide.ui.io.navigator.FileTreeContentProvider"
            icon="icons/full/obj16/projects.png"
            id="com.aptana.ide.ui.io.navigatorContent"
            labelProvider="com.aptana.ide.ui.io.navigator.FileTreeLabelProvider"
            name="Navigator Content"
            priority="high"
            providesSaveables="true">
         <enablement>
            <or>
               <instanceof
                     value="org.eclipse.core.resources.IWorkspaceRoot">
               </instanceof>
               <adapt
                     type="org.eclipse.core.resources.IProject">
               </adapt>
               <instanceof
                     value="com.aptana.ide.core.io.IConnectionPointCategory">
               </instanceof>
               <instanceof
                     value="com.aptana.ide.core.io.IConnectionPoint">
               </instanceof>
               <instanceof
                     value="com.aptana.ide.ui.io.navigator.LocalFileSystems">
               </instanceof>
               <instanceof
                     value="com.aptana.ide.core.io.LocalRoot">
               </instanceof>
               <instanceof
                     value="com.aptana.ide.ui.io.navigator.FileSystemObject">
               </instanceof>
               <instanceof
                     value="com.aptana.ide.ui.io.navigator.WorkspaceProjects">
               </instanceof>
               <instanceof
                     value="org.eclipse.core.resources.IResource">
               </instanceof>
            </or>
         </enablement>
         <commonSorter
               class="com.aptana.ide.ui.io.navigator.FileTreeNameSorter">
            <parentExpression>
               <or>
               <instanceof
                     value="org.eclipse.core.resources.IWorkspaceRoot">
               </instanceof>
               <instanceof
                     value="org.eclipse.core.resources.IResource">
               </instanceof>
                  <instanceof
                        value="com.aptana.ide.core.io.IConnectionPointCategory">
                  </instanceof>
               <instanceof
                     value="com.aptana.ide.core.io.IConnectionPoint">
               </instanceof>
               <instanceof
                     value="com.aptana.ide.ui.io.navigator.LocalFileSystems">
               </instanceof>
               <instanceof
                     value="com.aptana.ide.core.io.LocalRoot">
               </instanceof>
               <instanceof
                     value="com.aptana.ide.ui.io.navigator.FileSystemObject">
               </instanceof>
               <instanceof
                     value="com.aptana.ide.ui.io.navigator.WorkspaceProjects">
               </instanceof>
               </or>
            </parentExpression>
         </commonSorter>
         <possibleChildren>
            <or>
               <instanceof
                     value="com.aptana.ide.core.io.IConnectionPointCategory">
               </instanceof>
               <instanceof
                     value="com.aptana.ide.core.io.IConnectionPoint">
               </instanceof>
               <instanceof
                     value="com.aptana.ide.ui.io.navigator.LocalFileSystems">
               </instanceof>
               <instanceof
                     value="com.aptana.ide.core.io.LocalRoot">
               </instanceof>
               <instanceof
                     value="com.aptana.ide.ui.io.navigator.FileSystemObject">
               </instanceof>
               <instanceof
                     value="com.aptana.ide.ui.io.navigator.WorkspaceProjects">
               </instanceof>
            </or>
         </possibleChildren>
         <dropAssistant
               class="org.eclipse.ui.navigator.resources.ResourceDropAdapterAssistant"
               id="com.aptana.ide.ui.io.navigator.resources.resourceDropAdapter">
            <possibleDropTargets>
               <or>
                  <adapt
                        type="org.eclipse.core.resources.IProject">
                  </adapt>
                  <adapt
                        type="org.eclipse.core.resources.IFolder">
                  </adapt>
                  <adapt
                        type="org.eclipse.core.resources.IFile">
                  </adapt>
               </or>
            </possibleDropTargets>
         </dropAssistant>
         <actionProvider
               class="org.eclipse.ui.internal.navigator.resources.actions.EditActionProvider"
               id="com.aptana.ide.ui.io.navigator.actions.EditActions">
            <enablement>
               <or>
                  <adapt
                        type="org.eclipse.core.resources.IFile">
                  </adapt>
                  <adapt
                        type="org.eclipse.core.resources.IFolder">
                  </adapt>
                  <adapt
                        type="org.eclipse.core.resources.IProject">
                  </adapt>
                  <adapt
                        type="org.eclipse.core.resources.IWorkspaceRoot">
                  </adapt>
               </or>
            </enablement>
         </actionProvider>
         <actionProvider
               class="org.eclipse.ui.internal.navigator.resources.actions.RefactorActionProvider"
               id="com.aptana.ide.ui.io.navigator.actions.RefactorActions">
         </actionProvider>
         <commonWizard
               type="new"
               wizardId="org.eclipse.ui.wizards.new.folder">
            <enablement>
               <or>
                  <adapt
                        type="org.eclipse.core.resources.IFile">
                  </adapt>
                  <adapt
                        type="org.eclipse.core.resources.IFolder">
                  </adapt>
                  <adapt
                        type="org.eclipse.core.resources.IProject">
                  </adapt>
                  <adapt
                        type="org.eclipse.core.resources.IWorkspaceRoot">
                  </adapt>
               </or>
            </enablement>
         </commonWizard>
         <commonWizard
               type="new"
               wizardId="org.eclipse.ui.wizards.new.file">
            <enablement>
               <or>
                  <adapt
                        type="org.eclipse.core.resources.IFile">
                  </adapt>
                  <adapt
                        type="org.eclipse.core.resources.IFolder">
                  </adapt>
                  <adapt
                        type="org.eclipse.core.resources.IProject">
                  </adapt>
                  <adapt
                        type="org.eclipse.core.resources.IWorkspaceRoot">
                  </adapt>
               </or>
            </enablement>
         </commonWizard>
         <dropAssistant
               class="com.aptana.ide.ui.io.navigator.resources.FileDropAdapterAssistant"
               id="com.aptana.ide.ui.io.navigator.resources.fileDropAdapter">
            <possibleDropTargets>
               <or>
                  <instanceof
                        value="com.aptana.ide.ui.io.navigator.FileSystemObject">
                  </instanceof>
                  <instanceof
                        value="com.aptana.ide.core.io.LocalRoot">
                  </instanceof>
                  <instanceof
                        value="com.aptana.ide.core.io.IConnectionPoint">
                  </instanceof>
               </or></possibleDropTargets>
         </dropAssistant>
      </navigatorContent>
      <actionProvider
            class="com.aptana.ide.ui.io.navigator.actions.OpenActionProvider"
            id="com.aptana.ide.ui.io.navigator.actions.OpenActions">
         <enablement>
            <instanceof
                  value="com.aptana.ide.ui.io.navigator.FileSystemObject">
            </instanceof>
            <test
                  property="com.aptana.ide.ui.io.navigator.file.isDirectory"
                  value="false">
            </test>
         </enablement>
      </actionProvider>
      <actionProvider
            class="com.aptana.ide.ui.io.navigator.actions.NewProjectActionProvider"
            id="com.aptana.ide.ui.io.navigator.actions.NewActions">
         <enablement>
            <instanceof
                  value="com.aptana.ide.ui.io.navigator.WorkspaceProjects">
            </instanceof>
         </enablement>
      </actionProvider>
      <actionProvider
            class="com.aptana.ide.ui.io.navigator.actions.FileSystemNewActionProvider"
            id="com.aptana.ide.ui.io.navigator.actions.FileSystemNewActions">
         <enablement>
            <or>
               <instanceof
                     value="com.aptana.ide.core.io.LocalRoot">
               </instanceof>
               <instanceof
                     value="com.aptana.ide.ui.io.navigator.FileSystemObject">
               </instanceof>
            </or>
         </enablement>
      </actionProvider>
      <actionProvider
            class="com.aptana.ide.ui.io.navigator.actions.FileSystemEditActionProvider"
            id="com.aptana.ide.ui.io.navigator.actions.FileSystemEditActions">
         <enablement>
            <instanceof
                  value="com.aptana.ide.ui.io.navigator.FileSystemObject">
            </instanceof>
         </enablement>
      </actionProvider>
      <actionProvider
            class="com.aptana.ide.ui.io.navigator.actions.FileSystemRefactorActionProvider"
            id="com.aptana.ide.ui.io.navigator.actions.FileSystemRefactorActions">
         <enablement>
            <instanceof
                  value="com.aptana.ide.ui.io.navigator.FileSystemObject">
            </instanceof>
         </enablement>
      </actionProvider>
      <actionProvider
            class="com.aptana.ide.ui.io.navigator.actions.FileSystemMgmtActionProvider"
            id="com.aptana.ide.ui.io.navigator.actions.FileSystemMgmtActions">
         <enablement>
            <or>
               <instanceof
                     value="com.aptana.ide.core.io.LocalRoot">
               </instanceof>
               <instanceof
                     value="com.aptana.ide.ui.io.navigator.FileSystemObject">
               </instanceof>
               <instanceof
                     value="com.aptana.ide.core.io.IConnectionPoint">
               </instanceof>
            </or>
         </enablement>
      </actionProvider>
      <actionProvider
            class="org.eclipse.ui.navigator.resources.GoIntoActionProvider"
            id="com.aptana.ide.ui.io.navigator.actions.FileSystemGoIntoActions">
         <enablement>
            <or>
               <instanceof
                     value="com.aptana.ide.core.io.LocalRoot">
               </instanceof>
               <and>
                  <instanceof
                        value="com.aptana.ide.ui.io.navigator.FileSystemObject">
                  </instanceof>
                  <test
                        property="com.aptana.ide.ui.io.navigator.file.isDirectory"
                        value="true">
                  </test>
               </and>
               <instanceof
                     value="com.aptana.ide.core.io.IConnectionPoint">
               </instanceof>
               <instanceof
                     value="com.aptana.ide.core.io.IConnectionPointCategory">
               </instanceof>
               <instanceof
                     value="com.aptana.ide.ui.io.navigator.WorkspaceProjects">
               </instanceof>
               <instanceof
                     value="com.aptana.ide.ui.io.navigator.LocalFileSystems">
               </instanceof>
            </or>
         </enablement>
      </actionProvider>
      <actionProvider
            class="com.aptana.ide.ui.io.navigator.actions.RemoteNewActionProvider"
            id="com.aptana.ide.ui.io.navigator.actions.RemoteNewActions"
            overrides="com.aptana.ide.ui.io.navigator.actions.FileSystemNewActions">
         <enablement>
            <or>
               <and>
                  <instanceof
                        value="com.aptana.ide.core.io.IConnectionPoint">
                  </instanceof>
                  <test
                        property="com.aptana.ide.core.io.connection.canDisconnect"
                        value="true">
                  </test>
               </and>
               <and>
                  <instanceof
                        value="com.aptana.ide.ui.io.navigator.FileSystemObject">
                  </instanceof>
                  <test
                        property="com.aptana.ide.ui.io.navigator.file.isLocal"
                        value="false">
                  </test>
               </and>
            </or>
         </enablement>
      </actionProvider>
      <actionProvider
            class="com.aptana.ide.ui.io.navigator.actions.ProjectsExtendedActionProvider"
            id="com.aptana.ide.ui.io.navigator.actions.ProjectsExtendedActions">
         <enablement>
            <instanceof
                  value="com.aptana.ide.ui.io.navigator.WorkspaceProjects">
            </instanceof>
         </enablement>
      </actionProvider>
      <actionProvider
            class="com.aptana.ide.ui.io.navigator.actions.LocalShortcutsExtendedActionProvider"
            id="com.aptana.ide.ui.io.navigator.actions.LocalShortcutsExtendedActions">
         <enablement>
            <and>
               <instanceof
                     value="com.aptana.ide.core.io.IConnectionPointCategory">
               </instanceof>
               <test
                     property="com.aptana.ide.core.io.connection.category.id"
                     value="com.aptana.ide.core.io.localShortcuts">
               </test>
            </and>
         </enablement>
      </actionProvider>
      <actionProvider
            class="com.aptana.ide.ui.io.navigator.actions.ProjectShortcutsExtendedActionProvider"
            id="com.aptana.ide.ui.io.navigator.actions.ProjectShortcutsExtendedActions">
         <enablement>
            <and>
               <instanceof
                     value="com.aptana.ide.core.io.IConnectionPointCategory">
               </instanceof>
               <test
                     property="com.aptana.ide.core.io.connection.category.id"
                     value="com.aptana.ide.core.io.projectShortcuts">
               </test>
            </and>
         </enablement>
      </actionProvider>
      <commonFilter
            activeByDefault="true"
            description="Show other connections"
            id="com.aptana.ide.ui.io.navigator.filters.generic"
            name="Other Connections Filter">
         <filterExpression>
            <and>
               <instanceof
                     value="com.aptana.ide.core.io.IConnectionPointCategory">
               </instanceof>
               <test
                     property="com.aptana.ide.core.io.connection.category.id"
                     value="com.aptana.ide.core.io.generic">
               </test>
            </and>
         </filterExpression>
      </commonFilter>
   </extension>
