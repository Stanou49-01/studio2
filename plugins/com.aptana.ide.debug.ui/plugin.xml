   </extension>
   <extension
         point="org.eclipse.ui.editorActions">
      <editorContribution
            id="com.aptana.ide.debug.ui.BreakpointRulerActions"
            targetID="com.aptana.ide.editors.HTMLEditor">
         <action
               actionID="RulerDoubleClick"
               class="org.eclipse.debug.ui.actions.RulerToggleBreakpointActionDelegate"
               id="com.aptana.ide.debug.ui.actions.htmlDoubleClickBreakpointAction"
               label="%action.label.toggleBreakpoint.html"
               style="push">
         </action>
      </editorContribution>
      <editorContribution
            id="com.aptana.ide.debug.ui.BreakpointRulerActions"
            targetID="com.aptana.ide.editors.JSEditor">
         <action
               actionID="RulerDoubleClick"
               class="org.eclipse.debug.ui.actions.RulerToggleBreakpointActionDelegate"
               id="com.aptana.ide.debug.ui.actions.jsDoubleClickBreakpointAction"
               label="%action.label.toggleBreakpoint.js"
               style="push">
         </action>
      </editorContribution>
      <editorContribution
            id="com.aptana.ide.debug.ui.BreakpointRulerActions"
            targetID="com.aptana.ide.editors.PHPEditor">
         <action
               actionID="RulerDoubleClick"
               class="org.eclipse.debug.ui.actions.RulerToggleBreakpointActionDelegate"
               id="com.aptana.ide.debug.ui.actions.phpDoubleClickBreakpointAction"
               label="%action.label.toggleBreakpoint.php"
               style="push"/>
      </editorContribution>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTabGroups">
      <launchConfigurationTabGroup
            class="com.aptana.ide.debug.internal.ui.launchConfigurations.JSLaunchConfigurationTabGroup"
            description="%launchConfigurationTabGroup.description.js"
            id="com.aptana.ide.debug.ui.jsLaunchConfigurationTabGroup"
            type="com.aptana.ide.debug.core.jsLaunchConfigurationType">
         <launchMode
               mode="debug"
               perspective="org.eclipse.debug.ui.DebugPerspective"/>
         <launchMode mode="run"/>
      </launchConfigurationTabGroup>
      <launchConfigurationTabGroup
            class="com.aptana.ide.debug.internal.ui.launchConfigurations.JSRemoteLaunchConfigurationTabGroup"
            description="%launchConfigurationTabGroup.description.jsremote"
            id="com.aptana.ide.debug.ui.jsRemoteLaunchConfigurationTabGroup"
            type="com.aptana.ide.debug.core.jsRemoteLaunchConfigurationType">
         <launchMode
               mode="debug"
               perspective="org.eclipse.debug.ui.DebugPerspective"/>
         <launchMode mode="run"/>
      </launchConfigurationTabGroup>
   </extension>
   <extension
         point="org.eclipse.debug.ui.debugModelPresentations">
      <debugModelPresentation
            class="com.aptana.ide.debug.internal.ui.JSDebugModelPresentation"
            id="com.aptana.ide.debug.core"/>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTypeImages">
      <launchConfigurationTypeImage
            configTypeID="com.aptana.ide.debug.core.jsLaunchConfigurationType"
            icon="icons/full/obj16/launch-httpServer.gif"
            id="com.aptana.ide.debug.ui.jsLaunchConfigurationTypeImage"/>
      <launchConfigurationTypeImage
            configTypeID="com.aptana.ide.debug.core.jsRemoteLaunchConfigurationType"
            icon="icons/full/obj16/launch-httpServer.gif"
            id="com.aptana.ide.debug.ui.jsRemoteLaunchConfigurationTypeImage"/>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchShortcuts">
      <shortcut
            class="com.aptana.ide.debug.internal.ui.JSLaunchShortcut"
            icon="icons/full/obj16/launch-httpServer.gif"
            id="com.aptana.ide.debug.ui.jsApplicationShortcut"
            label="%shortcut.label.js"
            modes="run,debug">
         <contextualLaunch>
            <contextLabel
                  label="%contextLabel.label.js"
                  mode="debug"/>
            <enablement>
               <with variable="selection">
                  <count value="1"/>
                  <iterate>
                     <or>
	                     <test
	                           property="org.eclipse.debug.ui.matchesPattern"
	                           value="*.html"/>
	                     <test
	                           property="org.eclipse.debug.ui.matchesPattern"
	                           value="*.xhtml"/>
	                     <test
	                           property="org.eclipse.debug.ui.matchesPattern"
	                           value="*.htm"/>
                     </or>
                  </iterate>
               </with>
            </enablement>
         </contextualLaunch>
      </shortcut>
   </extension>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="com.aptana.ide.editors.unified.UnifiedEditor"
            class="com.aptana.ide.debug.internal.ui.actions.RetargettableActionAdapterFactory">
         <adapter type="org.eclipse.debug.ui.actions.IToggleBreakpointsTarget"/>
         <adapter type="org.eclipse.debug.ui.actions.IRunToLineTarget"/>
      </factory>
      <factory
            adaptableType="com.aptana.ide.debug.core.JSLaunchConfigurationDelegate"
            class="com.aptana.ide.debug.internal.ui.ActiveResourcePathGetterAdapterFactory">
         <adapter type="com.aptana.ide.debug.core.IActiveResourcePathGetterAdapter"/>
      </factory>
      <factory
            adaptableType="com.aptana.ide.debug.core.JSDebugPlugin"
            class="com.aptana.ide.debug.internal.ui.EditorOpenAdapterFactory">
         <adapter type="com.aptana.ide.debug.core.IEditorOpenAdapter"/>
      </factory>
      <factory
            adaptableType="com.aptana.ide.editors.unified.hover.LexemeTextHover"
            class="com.aptana.ide.debug.internal.ui.hovers.DebugHoverAdapterFactory">
         <adapter type="org.eclipse.jface.text.ITextHover"/>
      </factory>
      <factory
            adaptableType="com.aptana.ide.debug.core.JSLaunchConfigurationHelper"
            class="com.aptana.ide.debug.internal.ui.WebBrowserEnumeratorAdapterFactory">
         <adapter type="java.util.Enumeration"/>
      </factory>
      <factory
            adaptableType="com.aptana.ide.debug.core.model.IJSScriptElement"
            class="com.aptana.ide.debug.internal.ui.util.SourceDisplayAdapter$Factory">
         <adapter
               type="org.eclipse.debug.ui.sourcelookup.ISourceDisplay">
         </adapter>
         <adapter type="org.eclipse.debug.internal.ui.contexts.provisional.ISourceDisplayAdapter"/>
      </factory>
      <factory
            adaptableType="com.aptana.ide.debug.core.model.ISourceLink"
            class="com.aptana.ide.debug.internal.ui.util.SourceDisplayAdapter$Factory">
         <adapter
               type="org.eclipse.debug.ui.sourcelookup.ISourceDisplay">
         </adapter>
         <adapter type="org.eclipse.debug.internal.ui.contexts.provisional.ISourceDisplayAdapter"/>
      </factory>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension targetID="com.aptana.ide.js.ui.WebPerspective">
         <actionSet id="org.eclipse.debug.ui.launchActionSet"/>
         <actionSet id="org.eclipse.debug.ui.breakpointActionSet"/>
         <actionSet id="com.aptana.ide.debug.ui.JSDebugActionSet"/>
      </perspectiveExtension>
      <perspectiveExtension targetID="org.eclipse.debug.ui.DebugPerspective">
         <actionSet id="com.aptana.ide.debug.ui.JSDebugActionSet"/>
         <view
               id="com.aptana.ide.debug.ui.scriptsView"
               relationship="stack"
               relative="org.eclipse.debug.ui.ExpressionView"
               visible="true"/>
         <viewShortcut id="com.aptana.ide.debug.ui.scriptsView"/>
         <viewShortcut id="com.aptana.ide.debug.ui.AJAXMonitorView"/>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.propertyPages">
  <page
        adaptable="true"
        class="com.aptana.ide.debug.internal.ui.propertypages.JSLineBreakpointPropertyPage"
        id="com.aptana.ide.debug.ui.propertypages.common"
        name="%page.name.common"
        objectClass="com.aptana.ide.debug.core.model.IJSLineBreakpoint"/>
   </extension>
   <extension
         point="org.eclipse.debug.core.statusHandlers">
      <statusHandler
            class="com.aptana.ide.debug.internal.ui.InstallDebuggerPromptStatusHandler"
            code="301"
            id="com.aptana.ide.debug.ui.installDebuggerPromptStatusHandler"
            plugin="com.aptana.ide.debug.core"/>
      <statusHandler
            class="com.aptana.ide.debug.internal.ui.LaunchDebuggerPromptStatusHandler"
            code="302"
            id="com.aptana.ide.debug.ui.launchBrowserPromptStatusHandler"
            plugin="com.aptana.ide.debug.core"/>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="false"
            category="org.eclipse.debug.ui"
            class="com.aptana.ide.debug.internal.ui.views.AJAXMonitorView"
            icon="icons/full/obj16/xhrmon.gif"
            id="com.aptana.ide.debug.ui.AJAXMonitorView"
            name="%AJAXMonitorView.name"/>
      <view
            allowMultiple="false"
            category="org.eclipse.debug.ui"
            class="com.aptana.ide.debug.internal.ui.views.ScriptsView"
            icon="icons/full/obj16/script.gif"
            id="com.aptana.ide.debug.ui.scriptsView"
            name="%scriptsView.name"/>
   </extension>
   <extension
         point="org.eclipse.ui.console.consolePatternMatchListeners">
      <consolePatternMatchListener
            class="com.aptana.ide.debug.internal.ui.console.JSConsoleTracker"
            id="com.aptana.ide.debug.ui.JSConsoleTracker"
            regex="(?i)\([^(]*\.\w+:\d+\)">
         <enablement>
            <or>
               <test
                     property="org.eclipse.debug.ui.processTypeTest"
                     value="javascript"/>
            </or>
         </enablement>
      </consolePatternMatchListener>
