<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="false"
            id="com.aptana.ide.debug.ui.setStartPageMenuContribution"
            objectClass="org.eclipse.core.resources.IFile">
         <action
               class="com.aptana.ide.debug.internal.ui.actions.DefaultWebPreviewActionDelegate"
               enablesFor="1"
               id="com.aptana.ide.debug.ui.DefaultWebPreviewAction"
               label="%action.label.setStartPage"
               menubarPath="additions">
         </action>
         <enablement>
            <test
                  property="com.aptana.ide.core.ui.isWebRunnable"
                  value="true"/>
         </enablement>
      </objectContribution>
      <objectContribution
            adaptable="false"
            id="com.aptana.ide.debug.ui.unsetStartPageMenuContribution"
            objectClass="org.eclipse.core.resources.IFile">
         <action
               class="com.aptana.ide.debug.internal.ui.actions.RemoveStartPageAction"
               enablesFor="1"
               id="com.aptana.ide.debug.ui.RemoveStartPageAction"
               label="%action.label.unsetStartPage"
               menubarPath="additions"/>
         <enablement>
            <test
                  property="com.aptana.ide.debug.ui.isStartPage"
                  value="true"/>
         </enablement>
      </objectContribution>
      <objectContribution
            adaptable="true"
            id="com.aptana.ide.debug.ui.jsBreakpointActions"
            objectClass="com.aptana.ide.debug.core.model.IJSLineBreakpoint">
         <action
               class="com.aptana.ide.debug.internal.ui.actions.BreakpointHitCountAction"
               enablesFor="1"
               helpContextId="hit_count_action_context"
               icon="icons/full/elcl16/sethitcount_co.gif"
               id="com.aptana.ide.debug.ui.actions.BreakpointHitCount"
               label="%action.label.hitcount"
               menubarPath="emptyBreakpointGroup"/>
         <action
               class="com.aptana.ide.debug.internal.ui.actions.BreakpointPropertiesAction"
               enablesFor="1"
               helpContextId="breakpoint_properties_action_context"
               id="com.aptana.ide.debug.ui.actions.BreakpointProperties"
               label="%action.label.properties"
               menubarPath="additions"/>
      </objectContribution>
   		<viewerContribution
           id="com.aptana.ide.debug.ui.RulerPopupActions"
           targetID="com.aptana.ide.editors.HTMLEditor#UnifiedRulerContext">
         <action
               class="com.aptana.ide.debug.internal.ui.actions.BreakpointPropertiesRulerActionDelegate"
               helpContextId="breakpoint_properties_action_context"
               id="com.aptana.ide.debug.ui.actions.htmlBreakpointPropertiesRulerActionDelegate"
               label="%action.label.breakpoint.properties.html"
               menubarPath="debug"/>
         <action
               label="%action.label.toggle.html"
               class="org.eclipse.debug.ui.actions.RulerToggleBreakpointActionDelegate"
               menubarPath="debug"
               id="com.aptana.ide.debug.ui.actions.htmlToggleBreakpointAction">
         </action>
      </viewerContribution>
   		<viewerContribution
           id="com.aptana.ide.debug.ui.RulerPopupActions"
           targetID="com.aptana.ide.editors.JSEditor#UnifiedRulerContext">
         <action
               class="com.aptana.ide.debug.internal.ui.actions.BreakpointPropertiesRulerActionDelegate"
               helpContextId="breakpoint_properties_action_context"
               id="com.aptana.ide.debug.ui.actions.jsBreakpointPropertiesRulerActionDelegate"
               label="%action.label.breakpoint.properties.js"
               menubarPath="debug"/>
         <action
               label="%action.label.toggle.js"
               class="org.eclipse.debug.ui.actions.RulerToggleBreakpointActionDelegate"
               menubarPath="debug"
               id="com.aptana.ide.debug.ui.actions.jsToggleBreakpointAction">
         </action>
      </viewerContribution>
   		<viewerContribution
           id="com.aptana.ide.debug.ui.RulerPopupActions"
           targetID="com.aptana.ide.editors.PHPEditor#UnifiedRulerContext">
         <action
               class="com.aptana.ide.debug.internal.ui.actions.BreakpointPropertiesRulerActionDelegate"
               helpContextId="breakpoint_properties_action_context"
               id="com.aptana.ide.debug.ui.actions.phpBreakpointPropertiesRulerActionDelegate"
               label="%action.label.breakpoint.properties.php"
               menubarPath="debug"/>
         <action
               label="%action.label.toggle.php"
               class="org.eclipse.debug.ui.actions.RulerToggleBreakpointActionDelegate"
               menubarPath="debug"
               id="com.aptana.ide.debug.ui.actions.phpToggleBreakpointAction">
         </action>
      </viewerContribution>
     <viewerContribution
           id="com.aptana.ide.debug.ui.debugViewerContribution"
           targetID="org.eclipse.debug.ui.DebugView">
        <action
              class="com.aptana.ide.debug.internal.ui.actions.SuspendOnActionDelegate"
              enablesFor="1"
              icon="icons/full/elcl16/suspend_ondebugger.gif"
              id="com.aptana.ide.debug.ui.actions.suspendOnDebuggerKeywords"
              label="%suspendOnDebuggerKeywords.label"
              menubarPath="additions"
              state="false"
              style="toggle"/>
        <action
              class="com.aptana.ide.debug.internal.ui.actions.SuspendOnActionDelegate"
              enablesFor="1"
              icon="icons/full/elcl16/suspend_onerror.gif"
              id="com.aptana.ide.debug.ui.actions.suspendOnErrors"
              label="%suspendOnErrors.label"
              menubarPath="additions"
              state="false"
              style="toggle"/>
        <action
              class="com.aptana.ide.debug.internal.ui.actions.SuspendOnActionDelegate"
              enablesFor="1"
              icon="icons/full/elcl16/suspend_onexception.gif"
              id="com.aptana.ide.debug.ui.actions.suspendOnExceptions"
              label="%suspendOnExceptions.label"
              menubarPath="additions"
              state="false"
              style="toggle"/>
        <visibility>
           <systemProperty
                 name="com.aptana.ide.debug.core.debuggerActive"
                 value="true"/>
        </visibility>
     </viewerContribution>
     <viewerContribution
           id="com.aptana.ide.debug.ui.ContextPopupActions"
           targetID="com.aptana.ide.editors.HTMLEditor#UnifiedEditorContext">
         <action
               class="com.aptana.ide.debug.internal.ui.actions.PopupInspectAction"
               definitionId="com.aptana.ide.debug.ui.commands.Inspect"
               enablesFor="+"
               helpContextId="inspect_action_context"
               icon="icons/full/etool16/inspect.gif"
               id="com.aptana.ide.debug.ui.actions.htmlPopup.Inspect"
               label="%InspectAction.label"
               menubarPath="debug">
            <enablement>
               <and>
                  <systemProperty
                        name="com.aptana.ide.debug.core.debuggerActive"
                        value="true"/>
                  <objectClass name="org.eclipse.jface.text.ITextSelection"/>
               </and>
            </enablement>
         </action>
         <action
               class="com.aptana.ide.debug.internal.ui.actions.WatchAction"
               enablesFor="+"
               helpContextId="watch_action_context"
               icon="icons/full/etool16/watch_exp.gif"
               id="com.aptana.ide.debug.ui.actions.htmlPopup.Watch"
               label="%WatchAction.label"
               menubarPath="debug">
            <enablement>
               <and>
                  <systemProperty
                        name="com.aptana.ide.debug.core.debuggerActive"
                        value="true"/>
                  <objectClass name="org.eclipse.jface.text.ITextSelection"/>
               </and>
            </enablement>
         </action>
         <action
               class="org.eclipse.debug.ui.actions.RunToLineActionDelegate"
               definitionId="org.eclipse.debug.ui.commands.RunToLine"
               helpContextId="run_to_line_action_context"
               icon="icons/full/elcl16/runtoline_co.gif"
               id="com.aptana.ide.debug.ui.actions.htmlPopup.RunToLine"
               label="%RunToLine.label"
               menubarPath="debug">
            <enablement>
               <and>
                  <systemProperty
                        name="com.aptana.ide.debug.core.debuggerActive"
                        value="true"/>
                  <objectClass name="org.eclipse.jface.text.ITextSelection"/>
               </and>
            </enablement>
         </action>
         <visibility>
            <systemProperty
                  name="com.aptana.ide.debug.core.debuggerActive"
                  value="true"/>
         </visibility>
     </viewerContribution>
     <viewerContribution
           id="com.aptana.ide.debug.ui.ContextPopupActions"
           targetID="com.aptana.ide.editors.JSEditor#UnifiedEditorContext">
         <action
               class="com.aptana.ide.debug.internal.ui.actions.PopupInspectAction"
               definitionId="com.aptana.ide.debug.ui.commands.Inspect"
               enablesFor="+"
               helpContextId="inspect_action_context"
               icon="icons/full/etool16/inspect.gif"
               id="com.aptana.ide.debug.ui.actions.jsPopup.Inspect"
               label="%InspectAction.label"
               menubarPath="debug">
            <enablement>
               <and>
                  <systemProperty
                        name="com.aptana.ide.debug.core.debuggerActive"
                        value="true"/>
                  <objectClass name="org.eclipse.jface.text.ITextSelection"/>
               </and>
            </enablement>
         </action>
         <action
               class="com.aptana.ide.debug.internal.ui.actions.WatchAction"
               enablesFor="+"
               helpContextId="watch_action_context"
               icon="icons/full/etool16/watch_exp.gif"
               id="com.aptana.ide.debug.ui.actions.jsPopup.Watch"
               label="%WatchAction.label"
               menubarPath="debug">
            <enablement>
               <and>
                  <systemProperty
                        name="com.aptana.ide.debug.core.debuggerActive"
                        value="true"/>
                  <objectClass name="org.eclipse.jface.text.ITextSelection"/>
               </and>
            </enablement>
         </action>
         <action
               class="org.eclipse.debug.ui.actions.RunToLineActionDelegate"
               definitionId="org.eclipse.debug.ui.commands.RunToLine"
               helpContextId="run_to_line_action_context"
               icon="icons/full/elcl16/runtoline_co.gif"
               id="com.aptana.ide.debug.ui.actions.jsPopup.RunToLine"
               label="%RunToLine.label"
               menubarPath="debug">
            <enablement>
               <and>
                  <systemProperty
                        name="com.aptana.ide.debug.core.debuggerActive"
                        value="true"/>
                  <objectClass name="org.eclipse.jface.text.ITextSelection"/>
               </and>
            </enablement>
         </action>
         <visibility>
            <systemProperty
                  name="com.aptana.ide.debug.core.debuggerActive"
                  value="true"/>
         </visibility>
     </viewerContribution>
     <viewerContribution
           id="com.aptana.ide.debug.ui.ContextPopupActions"
           targetID="com.aptana.ide.editors.PHPEditor#UnifiedEditorContext">
         <action
               class="com.aptana.ide.debug.internal.ui.actions.PopupInspectAction"
               definitionId="com.aptana.ide.debug.ui.commands.Inspect"
               enablesFor="+"
               helpContextId="inspect_action_context"
               icon="icons/full/etool16/inspect.gif"
               id="com.aptana.ide.debug.ui.actions.phpPopup.Inspect"
               label="%InspectAction.label"
               menubarPath="debug">
            <enablement>
               <and>
                  <systemProperty
                        name="com.aptana.ide.debug.core.debuggerActive"
                        value="true"/>
                  <objectClass name="org.eclipse.jface.text.ITextSelection"/>
               </and>
            </enablement>
         </action>
         <action
               class="com.aptana.ide.debug.internal.ui.actions.WatchAction"
               enablesFor="+"
               helpContextId="watch_action_context"
               icon="icons/full/etool16/watch_exp.gif"
               id="com.aptana.ide.debug.ui.actions.phpPopup.Watch"
               label="%WatchAction.label"
               menubarPath="debug">
            <enablement>
               <and>
                  <systemProperty
                        name="com.aptana.ide.debug.core.debuggerActive"
                        value="true"/>
                  <objectClass name="org.eclipse.jface.text.ITextSelection"/>
               </and>
            </enablement>
         </action>
         <action
               class="org.eclipse.debug.ui.actions.RunToLineActionDelegate"
               definitionId="org.eclipse.debug.ui.commands.RunToLine"
               helpContextId="run_to_line_action_context"
               icon="icons/full/elcl16/runtoline_co.gif"
               id="com.aptana.ide.debug.ui.actions.phpPopup.RunToLine"
               label="%RunToLine.label"
               menubarPath="debug">
            <enablement>
               <and>
                  <systemProperty
                        name="com.aptana.ide.debug.core.debuggerActive"
                        value="true"/>
                  <objectClass name="org.eclipse.jface.text.ITextSelection"/>
               </and>
            </enablement>
         </action>
         <visibility>
            <systemProperty
                  name="com.aptana.ide.debug.core.debuggerActive"
                  value="true"/>
         </visibility>
     </viewerContribution>
     <objectContribution
           adaptable="false"
           id="com.aptana.ide.debug.ui.FilteredJSVariableActions"
           objectClass="com.aptana.ide.debug.core.model.IJSVariable">
        <action
              class="com.aptana.ide.debug.internal.ui.actions.NewDetailFormatterAction"
              enablesFor="1"
              helpContextId="new_detail_formatter_action_context"
              id="com.aptana.ide.debug.ui.actions.NewDetailFormatter"
              label="%newDetailFormatter.label"
              menubarPath="emptyNavigationGroup"
              tooltip="%newDetailFormatter.tooltip"/>
        <visibility>
           <not>
              <and>
                 <objectState
                       name="PrimitiveVariableActionFilter"
                       value="isValuePrimitive"/>
                 <objectState
                       name="DetailFormatterFilter"
                       value="isDefined"/>
              </and>
           </not>
        </visibility>
     </objectContribution>
     <objectContribution
           adaptable="false"
           id="com.aptana.ide.debug.ui.FilteredJSVariableActions"
           objectClass="com.aptana.ide.debug.core.model.IJSVariable">
        <action
              class="com.aptana.ide.debug.internal.ui.actions.RemoveDetailFormatterAction"
              enablesFor="1"
              helpContextId="remove_detail_formatter_action_context"
              id="com.aptana.ide.debug.ui.actions.RemoveDetailFormatter"
              label="%removeDetailFormatter.label"
              menubarPath="emptyNavigationGroup"
              tooltip="%removeDetailFormatter.tooltip"/>
        <action
              class="com.aptana.ide.debug.internal.ui.actions.EditDetailFormatterAction"
              enablesFor="1"
              helpContextId="edit_detail_formatter_action_context"
              id="com.aptana.ide.debug.ui.actions.EditDetailFormatter"
              label="%editDetailFormatter.label"
              menubarPath="emptyNavigationGroup"
              tooltip="%editDetailFormatter.tooltip"/>
        <visibility>
           <objectState
                 name="DetailFormatterFilter"
                 value="isDefined"/>
        </visibility>
     </objectContribution>
