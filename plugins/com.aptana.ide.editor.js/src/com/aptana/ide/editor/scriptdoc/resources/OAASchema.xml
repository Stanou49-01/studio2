		<child-element name="description" />
		<child-element name="examples" />
		<child-element name="remarks" />
		<child-element name="reference" />
		<child-element name="references" />
		<child-element name="title" />
	</element>
	
	<element name="options">
		<child-element name="option"/>
	</element>
	
	<element name="parameter">
		<attribute name="datatype" usage="optional"/>
		<attribute name="name"/>
		<attribute name="usage" usage="optional"/>
		<!-- descriptive elements -->
		<child-element name="description" />
		<child-element name="examples" />
		<child-element name="remarks" />
		<child-element name="reference" />
		<child-element name="references" />
		<child-element name="title" />
	</element>
	
	<element name="parameters">
		<child-element name="parameter"/>
	</element>
	
	<element name="property">
		<attribute name="datatype" usage="optional"/>
		<attribute name="default_attribute"/>
		<attribute name="hidden"/>
		<attribute name="name"/>
		<attribute name="readonly"/>
		<attribute name="required"/>
		<attribute name="scope"/>
		<attribute name="transient"/>
		<attribute name="urlparam"/>
		<attribute name="visibility"/>
		<child-element name="option"/>
		<child-element name="options"/>
		<!-- descriptive elements -->
		<child-element name="description" />
		<child-element name="examples" />
		<child-element name="remarks" />
		<child-element name="reference" />
		<child-element name="references" />
		<child-element name="title" />
		<!-- compatibility elements -->
		<child-element name="available" />
		<child-element name="deprecated" />
		<child-element name="useragents" />
	</element>
	
	<element name="properties">
		<child-element name="property"/>
	</element>
	
	<element name="quote" hasText="yes">
		<attribute name="locid" usage="optional"/>
		<attribute name="type" usage="optional"/>
	</element>
	
	<element name="remarks" hasText="yes">
		<attribute name="locid" usage="optional"/>
		<attribute name="type" usage="optional"/>
	</element>
	
	<element name="reference" hasText="yes">
		<attribute name="locid" usage="optional"/>
		<attribute name="type" usage="optional"/>
	</element>
	
	<element name="references">
		<child-element name="reference"/>
	</element>
	
	<element name="returns">
		<attribute name="datatype" usage="optional"/>
		<!-- descriptive elements -->
		<child-element name="description" />
		<child-element name="examples" />
		<child-element name="remarks" />
		<child-element name="reference" />
		<child-element name="references" />
		<child-element name="title" />
	</element>
	
	<element name="title" hasText="yes">
		<attribute name="locid" usage="optional"/>
		<attribute name="type" usage="optional"/>
	</element>
	
	<element name="useragent" hasText="yes">
		<attribute name="locid" usage="optional"/>
		<attribute name="os" usage="optional"/>
		<attribute name="osVersion" usage="optional"/>
		<attribute name="platform"/> <!-- usage="optional"/> -->
		<attribute name="version" usage="optional"/>
		<attribute name="type" usage="optional"/>
	</element>
	
	<element name="useragents">
		<child-element name="useragent"/>
	</element>
